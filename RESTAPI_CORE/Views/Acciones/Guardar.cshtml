@model InscriptosModel
@{
    ViewData["Title"] = "Guardar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
  <div class="card-header">
    Crear Contacto
  </div>
  <div class="card-body">
    

      <form asp-action="Guardar" asp-controller="Inscripcion" method="post">
      <div class="mb-3">
        <label class="form-label">Nombre</label>
        <input asp-for="Nombre" type="text" class="form-control">
        <span asp-validation-for="Nombre" class="text-danger"></span>
      </div>
            <div class="mb-3">
                <label class="form-label">Apellido</label>
                <input asp-for="Apellido" type="text" class="form-control">
                <span asp-validation-for="Apellido" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label class="form-label">DNI</label>
                <input asp-for="DNI" type="text" class="form-control">
                <span asp-validation-for="DNI" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label class="form-label">Edad</label>
                <input asp-for="Edad" type="text" class="form-control">
                <span asp-validation-for="Edad" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label class="form-label">Correo</label>
                <input asp-for="Correo" type="text" class="form-control">
                <span asp-validation-for="Correo" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label class="form-label">Carrrera</label>
                <input asp-for="Carrera" type="text" class="form-control">
                <span asp-validation-for="Carrera" class="text-danger"></span>
            </div>
      <button type="submit" class="btn btn-success">Guardar</button>
      <a asp-action="Listar" asp-controller="Inscripcion" class="btn btn-white">Volver a la lista</a>
    </form>


  </div>
</div>
